@inject OrzhansJozve.DataLayer.Repositories.IFooterLinkRepository _footerLinkRepository
<!DOCTYPE html>
<html class="no-js" lang="fa-IR">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
