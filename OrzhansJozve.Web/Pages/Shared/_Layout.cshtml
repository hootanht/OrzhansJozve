    <!--End Google Analytics-->
    <environment names="Staging,Production">
        <script defer async asp-append-version="true" src="~/js/site.min.js"></script>
    </environment>
    @await RenderSectionAsync("Scripts", false)
    <!-- Main js -->
    <environment names="Development">
        <script defer asp-append-version="true" src="~/js/main.js"></script>
    </environment>

    @*Organization*@

    <script defer type="application/ld+json">
        {
        "@@context" : "http://schema.org",
        "@@type" : "Organization",
        "name" : "orzhansjozve",
        "url" : "https://orzhansjozve.ir",
        "sameAs" : [
        "https://telegram.me/orzhansjozve",
        "https://www.instagram.com/orzhansjozveh",
        "https://www.aparat.com/orzhansjozve"
        ]
